/**
 * split 6-bit into lower and upper bits
 */

CHIP Split6 {
    IN in[6];
    OUT upper[3], lower[3];

    PARTS:
    // Put your code here:
    And(a=in[0], b=in[0], out=lower[0]);
    And(a=in[1], b=in[1], out=lower[1]);
    And(a=in[2], b=in[2], out=lower[2]);
    And(a=in[3], b=in[3], out=upper[0]);
    And(a=in[4], b=in[4], out=upper[1]);
    And(a=in[5], b=in[5], out=upper[2]);
}
